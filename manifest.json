{
    "description": "Display video cited in video inside youtube",
    "manifest_version": 2,
    "name": "Cited video",
    "version": "1.0",
    "homepage_url": "https://github.com/rousseau-romain/extension-cited-video/tree/master",
    "icons": {
        "48": "icons/icon.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "index.js"
            ],
            "run_at": "document_end"
        }
    ]
}