{
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "document": "writable"
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "no-multi-spaces": [
            "error",
            {
                "exceptions": {
                    "VariableDeclarator": true
                }
            }
        ],
        "key-spacing": [
            "error",
            {
                "align": "value"
            }
        ]
    }
}